<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        package="com.qolorquest">

	<uses-permission android:name="android.permission.VIBRATE"/>

    <application android:allowBackup="true"
        android:label="Q QUEST!"
        android:icon="@drawable/app_icon"
        android:name=".SketchApplication"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".DebugActivity" android:screenOrientation="portrait" />
			<activity android:name=".Main2Activity" android:screenOrientation="portrait"/>
			<activity android:name=".MenuActivity" android:screenOrientation="portrait"/>
			<activity android:name=".HelpActivity" android:screenOrientation="portrait"/>
			<activity android:name=".AboutActivity" android:screenOrientation="portrait"/>
			<activity android:name=".GameActivity" android:screenOrientation="portrait"/>
			<activity android:name=".CountdownActivity" android:screenOrientation="portrait"/>
			<activity android:name=".OverActivity" android:screenOrientation="portrait"/>
			<activity android:name=".ScoresActivity" android:screenOrientation="portrait"/>
			<activity android:name=".HighscoresActivity" android:screenOrientation="portrait"/>
			<activity android:name=".HighexpActivity" android:screenOrientation="portrait"/>

    </application>

</manifest>
